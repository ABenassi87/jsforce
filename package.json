{
  "author": "Shinichi Tomita <shinichi.tomita@gmail.com>",
  "name": "jsforce",
  "description": "Salesforce API Library for JavaScript",
  "keywords": [
    "salesforce",
    "salesforce.com",
    "sfdc",
    "force.com",
    "database.com"
  ],
  "homepage": "http://github.com/jsforce/jsforce",
  "version": "2.0.0-alpha",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsforce/jsforce.git"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/jsforce/jsforce/raw/master/LICENSE"
    }
  ],
  "module": "./module",
  "main": "./index",
  "types": "./lib.es6/index",
  "scripts": {
    "clean": "rimraf lib.es6/* module/*",
    "build:types": "run-p build:types:es5 build:types:module",
    "build:types:es5": "npm run build:types:src -- --outDir lib.es6",
    "build:types:module": "npm run build:types:src -- --outDir module",
    "build:types:src": "tsc --emitDeclarationOnly --declaration -p src",
    "build": "run-p build:types build:es5 build:module",
    "build:es5": "NODE_ENV=production npm run build:src -- -d lib.es6",
    "build:module": "NODE_ENV=module npm run build:src -- -d module",
    "build:src": "babel src --extensions \".ts\"",
    "lint": "eslint src test.es6",
    "typec": "tsc --noEmit",
    "test": "ava"
  },
  "directories": {
    "test": "test-ts/"
  },
  "files": [
    "README.md",
    "bower.json",
    "LICENSE",
    "package.json",
    "bin",
    "build",
    "lib",
    "test",
    "core.js",
    "index.js"
  ],
  "engines": {
    "node": ">=4.0"
  },
  "bin": {
    "jsforce": "./bin/jsforce"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "base64-url": "^2.2.0",
    "co-prompt": "^1.0.0",
    "coffeescript": "^1.10.0",
    "commander": "^2.9.0",
    "csv-parse": "^1.1.1",
    "csv-stringify": "^1.0.4",
    "faye": "^1.2.0",
    "inherits": "^2.0.1",
    "lodash": "^4.11.1",
    "multistream": "^2.0.5",
    "opn": "^5.3.0",
    "promise": "^7.1.1",
    "readable-stream": "^2.1.0",
    "request": "^2.72.0",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/csv-parse": "^1.1.11",
    "@types/node": "^10.11.7",
    "@types/puppeteer": "^1.11.1",
    "@types/request": "^2.47.1",
    "@types/through2": "^2.0.34",
    "ava": "^1.0.0-beta.8",
    "babel-eslint": "^7.1.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-typescript": "^0.12.0",
    "esm": "^3.0.82",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^1.6.2",
    "rimraf": "^2.6.2",
    "through2": "^2.0.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.2",
    "typescript-eslint-parser": "^20.0.0"
  }
}
